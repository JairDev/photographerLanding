<Layout title="Andrea Fotografía">
  <div class="header">
    <div class="jacob-gr-nberg">JACOB GRÖNBERG</div>
    <img class="menu-toggle" src="img/menu-toggle.svg" />
  </div>
  <main>
    <section class="hero">
      <div class="overlap">
        <div class="overlap-group">
          <div class="headline">
            <div class="jacob-gr-nberg">Jacob <br />Grønberg</div>
            <div class="photographer">PHOTOGRAPHER</div>
            <!-- <div class="visual-artist">&amp; VISUAL ARTIST</div> -->
          </div>
          <div class="image-wrapper">
            <img class="sergey-vinogradov" src={projects.image} />
          </div>
        </div>
        <div class="image">
          <div class="div">
            <div class="image-2">
              <img class="image-client" src={projects.image} />
            </div>
            <div class="text-wrapper">Hi, Im Jacob</div>
          </div>
          <div class="button">
            <div class="overlap-group-2">
              <div class="circle">
