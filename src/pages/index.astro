    </section>

    <section class="latest-work">
      <div class="grid">
        <div class="title">
          <div class="latest">LATEST</div>
          <div class="text-wrapper">Work</div>
        </div>
        <div class="button-text">
          <div class="arrow-right">View All</div>
          <img class="icon" src="img/icon.png" />
        </div>
      </div>
      <div class="projects">
        {
          pro.map((project) => (
            <div class="project">
              <div class="overlap-group-wrapper">
                <div class="dancing-in-black">{project.title}</div>
