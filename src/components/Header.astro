    width: fit-content;
    margin-top: -1px;
    font-family: var(--heading-uppercase-large-font-family);
    font-weight: var(--heading-uppercase-large-font-weight);
    color: var(--textprimary);
    font-size: var(--heading-uppercase-large-font-size);
    letter-spacing: var(--heading-uppercase-large-letter-spacing);
    line-height: var(--heading-uppercase-large-line-height);
    white-space: nowrap;
    font-style: var(--heading-uppercase-large-font-style);
  }

  .menu .menu-toggle {
    position: absolute;
    width: 200px;
    height: 200px;
    top: -50px;
    right: -50px;
    border-radius: 100px;
    border: 1px solid;
    border-color: var(--borderprimary);
  }

  .menu .overlap-group {
    position: relative;
    width: 36px;
    height: 36px;
    top: 82px;
    left: 82px;
  }

  .menu .line {
    position: absolute;
    width: 50px;
    top: 18px;
    left: -7px;
    transform: rotate(-45deg);
    height: 1px;
    background-color: var(--bordersecondary);
  }

  .menu .line-2 {
    position: absolute;
    width: 50px;
    top: 18px;
    left: -7px;
    transform: rotate(45deg);
    height: 1px;
    background-color: var(--bordersecondary);
  }

  .menu .menu-bttom {
    display: flex;
    /* width: 1600px; */
    align-items: center;
    justify-content: space-between;
    padding: 4rem 0rem 2rem;
  }

  .menu .column {
    display: inline-flex;
    align-items: center;
    gap: 10px;
    position: relative;
    flex: 0 0 auto;
  }

  .menu .text-wrapper-3 {
    position: relative;
    width: fit-content;
    margin-top: -1px;
    font-family: var(--text-default-font-family);
